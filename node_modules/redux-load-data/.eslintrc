extends: airbnb

plugins:
  - react
  - jsx-a11y
  - import

env:
  browser: true

rules:
  jsx-a11y/img-has-alt: off
  react/forbid-prop-types: off
  react/no-unused-prop-types: off
  react/jsx-filename-extension:
    - error
    - extensions:
      - .js
  no-plusplus:
    - error
    - allowForLoopAfterthoughts: true
  no-param-reassign:
    - error
    - props: false
